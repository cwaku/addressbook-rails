<div id="<%= dom_id contact %>">
  <p>
    <strong>Firstname:</strong>
    <%= contact.firstname %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= contact.lastname %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Remarks:</strong>
    <%= contact.remarks %>
  </p>

  <p>
    <strong>User:</strong>
    <%= contact.user_id %>
  </p>

  <p>
    <strong>Suburb:</strong>
    <%= contact.suburb_id %>
  </p>

</div>
